<div class="main-wrapper">
  <div class="content">
	<% if @players.length > 0 %>
		<% @players.each do |player| %>
			<div class="elem">
				<label>IP:</label><span><%= player.ip %></span>
				<label>EMAIL:</label><span><%= player.email %></span>
			</div>
		<% end %>
	<% else %>
		<p>No players</p>
	<% end %>
	<div class="c"></div>
  </div>
