<div class="main-wrapper">
  <div class="content">
    <div class="moves">
      <h1>Moves of game: <%= params[:game_id] %></h1>
      <% if @moves.length > 0 %>
        <table class="table table-striped table-hover">
          <thead>
            <tr>
              <th>Turn</th>
              <th>Player</th>
              <th>Data</th>
            </tr>
          </thead>
          <tbody>
            <% @moves.each do |move| %>
              <tr>
                <td><%= move.turn %></td>
                <td><%= move.player.email %></td>
                <td><%= move.data %></td>
              </tr> 
            <% end %>
          </tbody>
        </table>
      <% else %>
        <p>No moves</p>
      <% end %>
    </div>
  </div>
